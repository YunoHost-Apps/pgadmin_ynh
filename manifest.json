{
    "name": "pgAdmin",
    "id": "pgadmin",
    "packaging_format": 1,
    "description": {
        "en": "Manage PostgreSQL databases over the web",
        "fr": "Application web de gestion des bases de données PostgreSQL"
    },
    "version": "4-6.12~ynh1",
    "url": "https://www.pgadmin.org",
    "upstream": {
        "license": "PostgreSQL",
        "website": "https://www.pgadmin.org",
        "admindoc": "https://www.pgadmin.org/docs"
    },
    "license": "PostgreSQL",
    "maintainer": {
        "name": "Josué Tille",
        "email": "josue@tille.ch"
    },
    "requirements": {
        "yunohost": ">= 4.3.0"
    },
    "multi_instance": false,
    "services": [
        "nginx",
        "uwsgi"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/pgadmin",
                "default": "/pgadmin"
            },
            {
                "name": "admin",
                "type": "user"
            },
            {
                "name": "password",
                "type": "password"
            }
        ]
    }
}
